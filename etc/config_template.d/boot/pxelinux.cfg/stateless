# default is label '__IMAGE__'
# boots a Linux kernel and mounts rootfs.gz as a root file system on a 512MiB ramdisk
default __IMAGE__

label   __IMAGE__
    kernel images/__IMAGE__/vmlinuz
        append initrd=images/__IMAGE__/initrd.img overlay_rootfs=__OVERLAY__PROTOCOL__://__OVERLAY_SERVER__/images/__IMAGE__/rootfs.squashfs overlay_type=squashfs ip=dhcp netdevice=eth0 rd.neednet=1 __CONSOLE_OPTIONS__ rd.info
