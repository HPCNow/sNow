# default is label '__IMAGE__'
# boots a Linux kernel and mounts rootfs.gz as a root file system on a 512MiB ramdisk
default __IMAGE__

label   __IMAGE__
    kernel images/__IMAGE__/vmlinuz
        append initrd=images/__IMAGE__/initrd.img root=nfs:__NFS_SERVER__:/sNow/snow-configspace/boot/images/__IMAGE__/rootfs ro ip=dhcp netdevice=eth0 __CONSOLE_OPTIONS__
